{
	"name": "@tradeshift/tradeshift-app",
	"description": "Tradeshift App Messaging Library",
	"version": "0.0.0-development",
	"main": "lib/ts.app-cjs.js",
	"module": "lib/ts.app-esm.js",
	"browser": "lib/ts.app-umd.js",
	"types": "lib/ts.app.d.ts",
	"scripts": {
		"prebuild": "rm -rf ./lib/* && cp ./src/idx.d.ts ./lib/ts.app.d.ts",
		"build": "rollup -c",
		"dev": "rollup -c -w",
		"prelint": "prettier --write --config .prettierrc \"src/**/*.{js,less,json}\"",
		"lint": "eslint --config eslintrc.json --color src",
		"postlint": "prettier --write --config .prettierrc-md \"*.md\"",
		"pretest": "npm run lint && npm run build",
		"test": "echo \"No tests specified yet.\" && exit 0",
		"travis-deploy-once": "travis-deploy-once",
		"semantic-release": "semantic-release"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Tradeshift/tradeshift-app.git"
	},
	"author": "Daniel Spitzer <dsp@tradeshift.com>",
	"bugs": {
		"url": "https://github.com/Tradeshift/tradeshift-app/issues"
	},
	"homepage": "https://github.com/Tradeshift/tradeshift-app#readme",
	"devDependencies": {
		"@babel/cli": "^7.0.0-beta.52",
		"@babel/core": "^7.0.0-beta.52",
		"@babel/plugin-proposal-class-properties": "^7.0.0-beta.52",
		"@babel/plugin-transform-runtime": "^7.0.0-beta.52",
		"@babel/polyfill": "^7.0.0-beta.52",
		"@babel/preset-env": "^7.0.0-beta.52",
		"@babel/runtime": "^7.0.0-beta.52",
		"debug": "^3.1.0",
		"eslint": "5.1.0",
		"eslint-config-tradeshift": "5.1.0",
		"prettier": "1.13.7",
		"rollup": "^0.62.0",
		"rollup-plugin-babel": "^4.0.0-beta.7",
		"rollup-plugin-commonjs": "^9.1.3",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-node-globals": "^1.2.1",
		"rollup-plugin-node-resolve": "^3.3.0",
		"semantic-release": "^15.6.3",
		"travis-deploy-once": "^5.0.1"
	},
	"dependencies": {
		"uuid": "^3.3.2"
	}
}
